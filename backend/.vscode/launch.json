{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "env": {},
      "args": [],
      "showLog": true,
      "console": "integratedTerminal"
    },
    {
      "name": "Debug Current File",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${file}",
      "env": {},
      "args": [],
      "showLog": true,
      "console": "integratedTerminal"
    },
    {
      "name": "Debug with Environment Variables",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "env": {
        "DATABASE_URL": "postgres://username:password@localhost:5432/dbname?sslmode=disable"
      },
      "args": [],
      "showLog": true,
      "console": "integratedTerminal"
    }
  ]
}
